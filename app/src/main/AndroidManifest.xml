<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lt.unitreetest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ui.homepage.HomepageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.device.GuideActivity" />
        <activity android:name=".ui.personal.PasswordManagementActivity" />
        <activity android:name=".ui.personal.MyRecordActivity" />
        <activity android:name=".ui.personal.TechSupportActivity" />
        <activity android:name=".ui.personal.ForumActivity" />
        <activity
            android:name=".ui.device.ConsoleActivity"
            android:theme="@style/Transparent" />
        <activity android:name=".ui.personal.PasswordManagement.ModifyPasswordActivity" />
        <activity android:name=".ui.personal.PasswordManagement.BackPasswordActivity" />
        <activity android:name=".ui.personal.ModifyInformationActivity" android:theme="@style/DefaultCityPickerTheme"/>
        <activity android:name=".ui.device.FirmwareUpdateActivity"></activity>
        <activity
            android:name="me.nereo.multi_image_selector.MultiImageSelectorActivity"
            android:configChanges="orientation|screenSize" />
    </application>

</manifest>